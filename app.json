{
  "name": "TestAdame",
  "slug": "TestAdame",
  "scheme": "testadame",
  "version": "1.0.0",
  "orientation": "portrait",
  "userInterfaceStyle": "automatic",
  "icon": "./assets/images/rick-and-morty-icon.png",
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "newArchEnabled": true,
  "jsEngine": "hermes",
  "assetBundlePatterns": [
    "**/*"
  ],
  "android": {
    "icon": "./assets/images/rick-and-morty-icon.png",
    "package": "com.macadam.testadame",
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/rick-and-morty-icon.png",
      "backgroundColor": "#191015"
    },
    "allowBackup": false,
    "edgeToEdgeEnabled": true
  },
  "ios": {
    "icon": "./assets/images/rick-and-morty-icon.png",
    "supportsTablet": true,
    "bundleIdentifier": "com.macadam.testadame",
    "appleTeamId": "8P4D8RH64J"
  },
  "web": {
    "favicon": "./assets/images/rick-and-morty-icon.png",
    "bundler": "metro"
  },
  "plugins": [
    "expo-localization",
    "expo-font",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/rick-and-morty-icon.png",
        "imageWidth": 300,
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    ],
    [
      "react-native-edge-to-edge",
      {
        "android": {
          "parentTheme": "Light",
          "enforceNavigationBarContrast": false
        }
      }
    ],
    "expo-build-properties"
  ],
  "experiments": {
    "tsconfigPaths": true
  },
  "extra": {
    "ignite": {
      "version": "11.3.2"
    }
  }
}
